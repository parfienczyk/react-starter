{
  "name": "react-kp-starter",
  "version": "0.1.3",
  "private": true,
  "author": "Karol Parfienczyk <parfienczyk@gmail.com> (https://parfienczyk.pl)",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint .",
    "pretty": "npx prettier --single-quote --write '**/*.{js,jsx,json,css,scss}'",
    "serve": "npx serve --no-clipboard --single --listen 5000 dist",
    "tree": "tree -I 'node_modules'",
    "__precommit": "pretty-quick --staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "^3.4.0",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "node-sass": "^4.10.0",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.6.0"
  },
  "__lint-staged": {
    "src/**/*.{js,jsx,json,css,scss}": [
      "prettier --single-quote --write",
      "eslint --cache --cache-location node_modules/.cache/eslint",
      "git add"
    ]
  },
  "__husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}